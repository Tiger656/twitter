{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "_____": "___________Versioning_____________",
    "doc_changes": "changeset add",
    "release": "changeset version && changeset tag && git push --follow-tags",
    "clean:dist": "rm -rf apps/*/dist && rm -rf packages/*/dist",
    "clean:node_modules": "rm -rf apps/*/node_modules && rm -rf packages/*/node_modules",
    "clean": "yarn clean:dist && yarn clean:node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "doppler": "doppler -p app_twitter_monorepo -c ${APP_ENV} run -- ",
    "env:local:up": "APP_ENV=local yarn doppler docker compose up -d",
    "env:local:down": "APP_ENV=local yarn doppler docker compose down ",
    "env:localprod:up": "APP_ENV=prod yarn doppler docker compose up -d",
    "env:localprod:down": "APP_ENV=prod yarn doppler docker compose down ",
    "env:test:up": "APP_ENV=test yarn doppler docker compose up -d",
    "env:test:down": "APP_ENV=test yarn doppler docker compose down"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  },
  "name": "my-turborepo",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}